#: libuser.c:91
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: error de inicialización de libuser:"

#: libuser.c:157
msgid "Warning: unlocked password would be empty."
msgstr "Advertencia: contraseña desbloqueada puede estar vacía."

#: libuser.c:255
msgid "Corrupted passwd entry."
msgstr "Entrada de passwd corrupta."

#: libuser.c:272
msgid "Empty password."
msgstr "Contraseña vacía."

#: libuser.c:275
msgid "Password locked."
msgstr "Contraseña bloqueada."

#: libuser.c:279
msgid "Password set, MD5 crypt."
msgstr "Contraseña establecida, cifrado MD5."

#: libuser.c:282
msgid "Password set, blowfish crypt."
msgstr "Contraseña establecida, cifrado blowfish"

#: libuser.c:285
msgid "Password set, SHA256 crypt."
msgstr "Contraseña establecida, cifrado SHA256."

#: libuser.c:288
msgid "Password set, SHA512 crypt."
msgstr "Contraseñaestablecida, cifrado SHA512."

#: libuser.c:290
msgid "Password set, unknown crypt variant."
msgstr "Contraseña establecida, variante de cifrado desconocida."

#: libuser.c:293
msgid "Alternate authentication scheme in use."
msgstr "Esquema de autenticación alternativo en uso."

#: libuser.c:298
msgid "Password set, DES crypt."
msgstr "Contraseña establecida, cifrado DES."

#: libuser.c:311
#, c-format
msgid "No password set.\n"
msgstr "No se establecio contraseña.\n"

#: libuser.c:316
#, c-format
msgid "Unknown user.\n"
msgstr "Usuario desconocido.\n"

#: libuser.c:391
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr ""
"%s: la cuenta del usuario no tiene soporte para envejecimiento de "
"contraseña.\n"

#: passwd.c:158
msgid "keep non-expired authentication tokens"
msgstr "mantener los tokens de autenticación no expirados"

#: passwd.c:160
msgid "delete the password for the named account (root only)"
msgstr "borrar la contraseña para la cuenta indicada (solo root)"

#: passwd.c:163
msgid "lock the password for the named account (root only)"
msgstr "bloquear la contraseña para la cuenta indicada (solo root)"

#: passwd.c:166
msgid "unlock the password for the named account (root only)"
msgstr "desbloquear la constraseña para la cuenta indicada (solo root)"

#: passwd.c:169
msgid "expire the password for the named account (root only)"
msgstr "expirar la contraseña para la cuenta indicada (solo root)"

#: passwd.c:172
msgid "force operation"
msgstr "forzar la operación"

#: passwd.c:174
msgid "maximum password lifetime (root only)"
msgstr "tiempo de vida máximo de la contraseña (solo root)"

#: passwd.c:176
msgid "minimum password lifetime (root only)"
msgstr "tiempo mínimo de vida de la contraseña (solo root)"

#: passwd.c:178
msgid ""
"number of days warning users receives before password expiration (root only)"
msgstr ""
"número de días antes del vencimiento de la contraseña en que los usuarios "
"reciben las advertencias de vencimiento (solo root)"

#: passwd.c:181
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr ""
"número de días después de que venció la contraseña en que se deshabilitará "
"la cuenta (solo root)"

#: passwd.c:184
msgid "report password status on the named account (root only)"
msgstr "informar el estado de la contraseña de la cuenta indicada (solo root)"

#: passwd.c:187
msgid "read new tokens from stdin (root only)"
msgstr "leer tokens nuevos desde stdin (solo root)"

#: passwd.c:194
msgid "[OPTION...] <accountName>"
msgstr "[OPCION...] <nombreDeCuenta>"

#: passwd.c:197
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: argumento incorrecto %s: %s\n"

#: passwd.c:258
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr "%s: solo uno de -l, -u, -d, -S se puede especificar.\n"

#: passwd.c:267
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr ""
"%s: No se pueden mezclar una de las opciones -l, -u, -d, -S y una de -i, -n, "
"-w, -x.\n"

#: passwd.c:286
#, c-format
msgid "Only root can do that.\n"
msgstr "Solo root puede hacer eso.\n"

#: passwd.c:299
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: solo root puede especificar un nombre de usuario.\n"

#: passwd.c:308
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: el nombre de usuario provisto es muy largo.\n"

#: passwd.c:318
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: solo se puede especificar un nombre de usuario.\n"

#: passwd.c:328
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: Esta opción requiere un nombre de usuario.\n"

#: passwd.c:339
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: ¡No puede identificarlo!\n"

#: passwd.c:348 passwd.c:383
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: Nombre de usuario desconocido '%s'.\n"

#: passwd.c:392
#, c-format
msgid "%s: SELinux denying access due to security policy.\n"
msgstr "%s: SELinux deniega el acceso por politica de seguridad.\n"

#: passwd.c:402
#, c-format
msgid "Locking password for user %s.\n"
msgstr "Bloqueando la contraseña para el usuario %s.\n"

#: passwd.c:406 passwd.c:418 passwd.c:433 passwd.c:444 passwd.c:464
msgid "Success"
msgstr "Éxito"

#: passwd.c:406 passwd.c:421
msgid "Error (password not set?)"
msgstr "Error (¿no se puso la contraseña?)"

#: passwd.c:414
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "Desbloqueando la contraseña para el usuario %s.\n"

#: passwd.c:420
msgid "Unsafe operation (use -f to force)"
msgstr "Operación insegura (use -f para forzarla)"

#: passwd.c:429
#, c-format
msgid "Expiring password for user %s.\n"
msgstr "Expirando contraseña para el usuario %s.\n"

#: passwd.c:433 passwd.c:444 passwd.c:464
msgid "Error"
msgstr "Error"

#: passwd.c:441
#, c-format
msgid "Removing password for user %s.\n"
msgstr "Eliminando la contraseña del usuario %s.\n"

#: passwd.c:461
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "Ajustando los datos de envejecimiento para el usuario %s.\n"

#: passwd.c:477
#, c-format
msgid "Changing password for user %s.\n"
msgstr "Cambiando la contraseña del usuario %s.\n"

#: passwd.c:489
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: error al leer de stdin: %s\n"

#: passwd.c:506
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: no se pudo iniciar pam: %s\n"

#: passwd.c:519
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: no se pudo poner el tty para pam: %s\n"

#: passwd.c:530
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr "%s: no se pudo poner la espera máxima para fallo: %s\n"

#: passwd.c:548
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr "%s: tokens de autenticación expirados actualizados con éxito.\n"

#: passwd.c:554
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr "%s: todos los tokens de autenticación se actualizaron exitosamente.\n"
