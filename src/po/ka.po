#: libuser.c:91
#, c-format
msgid "%s: libuser initialization error:"
msgstr "%s: შეცდომა libuser-ის ინიციალიზაციისას:"

#: libuser.c:157
msgid "Warning: unlocked password would be empty."
msgstr "ყურადღება: განბლოკილი პაროლი ცარიელი იქნება."

#: libuser.c:255
msgid "Corrupted passwd entry."
msgstr "დაზიანებული passwd ჩანაწერი."

#: libuser.c:272
msgid "Empty password."
msgstr "პაროლი ცარიელია."

#: libuser.c:275
msgid "Password locked."
msgstr "პაროლი დაბლოკილია."

#: libuser.c:279
msgid "Password set, MD5 crypt."
msgstr "დაყენებულია MD5-ით დაშიფრული პაროლი."

#: libuser.c:282
msgid "Password set, blowfish crypt."
msgstr "დაყენებულია blowfish-ით დაშიფრული პაროლი."

#: libuser.c:285
msgid "Password set, SHA256 crypt."
msgstr "დაყენებულია SHA256-ით დაშიფრული პაროლი."

#: libuser.c:288
msgid "Password set, SHA512 crypt."
msgstr "დაყენებულია SHA512-ით დაშიფრული პაროლი."

#: libuser.c:290
msgid "Password set, unknown crypt variant."
msgstr "დაყენებულია უცნობი ალგორითმით დაშიფრული პაროლი."

#: libuser.c:293
msgid "Alternate authentication scheme in use."
msgstr "გამოიყენება ავთენტიკაციის ალტერნატიული სქემა."

#: libuser.c:298
msgid "Password set, DES crypt."
msgstr "დაყენებულია DES-ით დაშიფრული პაროლი."

#: libuser.c:311
#, c-format
msgid "No password set.\n"
msgstr "პაროლი არაა დაყენებული.\n"

#: libuser.c:316
#, c-format
msgid "Unknown user.\n"
msgstr "უცნობი მომხმარებელი.\n"

#: libuser.c:391
#, c-format
msgid "%s: user account has no support for password aging.\n"
msgstr "%s: მომხმარებლის ანგარიშს არ აქვს პაროლის დაძველების მხარდაჭერა.\n"

#: passwd.c:158
msgid "keep non-expired authentication tokens"
msgstr ""

#: passwd.c:160
msgid "delete the password for the named account (root only)"
msgstr ""

#: passwd.c:163
msgid "lock the password for the named account (root only)"
msgstr ""

#: passwd.c:166
msgid "unlock the password for the named account (root only)"
msgstr ""

#: passwd.c:169
msgid "expire the password for the named account (root only)"
msgstr ""

#: passwd.c:172
msgid "force operation"
msgstr "ოპერაციის ძალდატანება"

#: passwd.c:174
msgid "maximum password lifetime (root only)"
msgstr "პაროლის მაქსიმალური ხანგრძლივობა (მხოლოდ root)"

#: passwd.c:176
msgid "minimum password lifetime (root only)"
msgstr "პაროლის მინიმალური ხანგრძლივობა (მხოლოდ root)"

#: passwd.c:178
msgid ""
"number of days warning users receives before password expiration (root only)"
msgstr ""

#: passwd.c:181
msgid ""
"number of days after password expiration when an account becomes disabled "
"(root only)"
msgstr ""

#: passwd.c:184
msgid "report password status on the named account (root only)"
msgstr ""

#: passwd.c:187
msgid "read new tokens from stdin (root only)"
msgstr ""

#: passwd.c:194
msgid "[OPTION...] <accountName>"
msgstr "[პარამეტრი...] <ანგარიშის სახელი>"

#: passwd.c:197
#, c-format
msgid "%s: bad argument %s: %s\n"
msgstr "%s: ცუდი არგუმენტი %s: %s\n"

#: passwd.c:258
#, c-format
msgid "%s: Only one of -l, -u, -d, -S may be specified.\n"
msgstr ""
"%s: პარამეტრებიდან -l, -u, -d, -S შესაძლებელია მხოლოდ ერთის მითითება.\n"

#: passwd.c:267
#, c-format
msgid "%s: Cannot mix one of -l, -u, -d, -S and one of -i, -n, -w, -x.\n"
msgstr ""
"%s: შეუძლებელია ერთი -l, -u, -d, -S და ერთი of -i, -n, -w, -x პარამეტრის "
"შერევა.\n"

#: passwd.c:286
#, c-format
msgid "Only root can do that.\n"
msgstr "მხოლოდ root-ს შეუძლია ამის გაკეთება.\n"

#: passwd.c:299
#, c-format
msgid "%s: Only root can specify a user name.\n"
msgstr "%s: მხოლოდ root-ს შეუძლია მომხმარებლის სახელის მითითება.\n"

#: passwd.c:308
#, c-format
msgid "%s: The user name supplied is too long.\n"
msgstr "%s: მითითებული მომხმარებლის სახელი ძალიან გრძელია.\n"

#: passwd.c:318
#, c-format
msgid "%s: Only one user name may be specified.\n"
msgstr "%s: შესაძლებელია მხოლოდ ერთი მომხმარებლის მითითება.\n"

#: passwd.c:328
#, c-format
msgid "%s: This option requires a user name.\n"
msgstr "%s: ეს პარამეტრი მოითხოვს მომხმარებლის სახელს.\n"

#: passwd.c:339
#, c-format
msgid "%s: Can not identify you!\n"
msgstr "%s: თქვენი იდენტურობა ვერ დადგინდა!\n"

#: passwd.c:348 passwd.c:383
#, c-format
msgid "%s: Unknown user name '%s'.\n"
msgstr "%s: მომხმარებლის სახელი '%s' უცნობია.\n"

#: passwd.c:392
#, c-format
msgid "%s: SELinux denying access due to security policy.\n"
msgstr "%s: უსაფრთხოების პოლიტიკიდან გამომდინარე SELinux კრძალავს წვდომას.\n"

#: passwd.c:402
#, c-format
msgid "Locking password for user %s.\n"
msgstr "პაროლის დაბლოკვა მომხმარებლისთვის %s.\n"

#: passwd.c:406 passwd.c:418 passwd.c:433 passwd.c:444 passwd.c:464
msgid "Success"
msgstr "წარმატებით დასრულდა"

#: passwd.c:406 passwd.c:421
msgid "Error (password not set?)"
msgstr "შეცდომა (პაროლი არ არის დანიშნული?)"

#: passwd.c:414
#, c-format
msgid "Unlocking password for user %s.\n"
msgstr "პაროლის განბლოკვა მომხმარებლისთვის %s.\n"

#: passwd.c:420
msgid "Unsafe operation (use -f to force)"
msgstr "საშიში ოპერაცია (დასაძალებლად გამოიყენეთ -f)"

#: passwd.c:429
#, c-format
msgid "Expiring password for user %s.\n"
msgstr ""

#: passwd.c:433 passwd.c:444 passwd.c:464
msgid "Error"
msgstr "შეცდომა"

#: passwd.c:441
#, c-format
msgid "Removing password for user %s.\n"
msgstr "%s მომხმარებლისთვის პაროლის მოხსნა.\n"

#: passwd.c:461
#, c-format
msgid "Adjusting aging data for user %s.\n"
msgstr "%s მომხმარებლის ვადების განსაზღვრა.\n"

#: passwd.c:477
#, c-format
msgid "Changing password for user %s.\n"
msgstr "%s მომხმარებლის პაროლის შეცვლა.\n"

#: passwd.c:489
#, c-format
msgid "%s: error reading from stdin: %s\n"
msgstr "%s: შეცდომა stdin-დან წაკითხვისას: %s\n"

#: passwd.c:506
#, c-format
msgid "%s: unable to start pam: %s\n"
msgstr "%s: pam-ის გაშვება ვერ მოხერხდა: %s\n"

#: passwd.c:519
#, c-format
msgid "%s: unable to set tty for pam: %s\n"
msgstr "%s: pam-თვის tty-ის დანიშვნა ვერ მოხერხდა: %s\n"

#: passwd.c:530
#, c-format
msgid "%s: unable to set failure delay: %s\n"
msgstr ""

#: passwd.c:548
#, c-format
msgid "%s: expired authentication tokens updated successfully.\n"
msgstr ""

#: passwd.c:554
#, c-format
msgid "%s: all authentication tokens updated successfully.\n"
msgstr ""
